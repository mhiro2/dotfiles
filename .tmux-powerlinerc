# General {
  # The theme to use.
  export TMUX_POWERLINE_THEME="mytheme"
  # Overlay directory to look for themes.
  export TMUX_POWERLINE_DIR_USER_THEMES="~/.tmux/themes"
# }

# weather.sh {
  # Use OpenWeatherMap API
  export TMUX_POWERLINE_SEG_WEATHER_LOCATION="Tokyo,JP"
  export TMUX_POWERLINE_SEG_WEATHER_UNIT="c"
# }
